{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [1, 4],
        "no-console": "off",
        "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
        "no-trailing-spaces": ["error", { "skipBlankLines": true, "ignoreComments": true }]
    }
}
